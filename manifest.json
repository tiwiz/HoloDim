{
    "name": "Holo Dim",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Calculates dimensions of your browser in Holo style",
    "browser_action":	{
        "default_icon": "images/icon128.png",
        "default_popup": "popup.html"
    },
    "icons":{
        "128":"images/icon128.png",
        "19": "images/icon19.png",          
        "38": "images/icon38.png"
    },
    "permissions": [ "tabs", "clipboardRead", "notifications", "storage" ],
    "default_locale":"en",
    "options_page": "options.html"

}